{
    "name": "yuges-code/laravel-ownable",
    "description": "ðŸ’¼ Package for easily attaching ownerships to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "owners",
        "laravel",
        "template",
        "ownerships"
    ],
    "homepage": "https://yuges-code.github.io/laravel-ownable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-ownable",
        "issues": "https://github.com/yuges-code/laravel-ownable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "yuges-code/laravel-package": "^1.0",
        "yuges-code/laravel-orderable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Ownable\\": "src",
            "Yuges\\Ownable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Ownable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Ownable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Ownable\\Providers\\OwnableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
